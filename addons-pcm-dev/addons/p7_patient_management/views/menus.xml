<odoo>
    <data>

        <!-- Associate menus -->
        <record id="action_associate_video_status" model="ir.actions.act_window">
            <field name="name">Video Call Status</field>
            <field name="res_model">patient.case</field>
            <field name="view_mode">tree,form</field>
            <field name="domain">[]</field>
            <field name="views" eval="[(ref('p7_patient_management.view_patient_case_associate_status_tree'), 'tree'), (ref('p7_patient_management.view_patient_case_form'), 'form')]"/>
        </record>

        <record id="action_associate_bookings_for_calling" model="ir.actions.act_window">
            <field name="name">Bookings for Calling</field>
            <field name="res_model">contact.attempt</field>
            <field name="view_mode">tree,form</field>
            <field name="domain">
                 [('associate_id','=',False),
                 ('video_call_assign_status','=','to_assign')]
            </field>
            <field name="view_id" ref="p7_patient_management.view_contact_attempt_bookings_tree"/>
            <field name="views" eval="[
                (ref('p7_patient_management.view_contact_attempt_bookings_tree'), 'tree'),
                (ref('p7_patient_management.view_contact_attempt_form'), 'form')
            ]"/>
            <field name="context">{'form_view_ref': 'p7_patient_management.view_contact_attempt_form'}</field>
        </record>

        <record id="action_associate_my_bookings" model="ir.actions.act_window">
            <field name="name">My Bookings</field>
            <field name="res_model">contact.attempt</field>
            <field name="view_mode">tree,form</field>
            <field name="domain">[('associate_id','=',uid),
                ('video_call_assign_status','in',['assigned','in_progress'])]
            </field>

            <!-- pin the correct tree view -->
            <field name="view_id" ref="p7_patient_management.view_contact_attempt_my_tree"/>

            <field name="views" eval="[
                (ref('p7_patient_management.view_contact_attempt_my_tree'), 'tree'),
                (ref('p7_patient_management.view_contact_attempt_my_booking_form'), 'form')
            ]"/>
            <field name="context">
                {
                'tree_view_ref': 'p7_patient_management.view_contact_attempt_my_tree',
                'form_view_ref': 'p7_patient_management.view_contact_attempt_my_booking_form'
                }
            </field>
        </record>


        <!-- reference template -->
        <record id="view_survey_survey_reference_tree_custom" model="ir.ui.view">
            <field name="name">survey.survey.reference.tree.custom</field>
            <field name="model">survey.survey</field>
            <field name="arch" type="xml">
                <tree>
                    <field name="title" string="Survey Title"/>
                    <field name="state" widget="badge" decoration-info="state == 'draft'" decoration-success="state == 'release'"/>
                    <field name="version"/>
                    <field name="released_date"/>
                    <button string="Use this Template"
                        type="object" name="create_personel_template" class="btn btn-primary"
                        invisible="state != 'release'"/>
                </tree>
            </field>
        </record>

        <record id="survey_user_input_view_kanban_custom" model="ir.ui.view">
            <field name="name">survey.user_input.view.kanban.custom</field>
            <field name="model">survey.user_input</field>
            <field name="arch" type="xml">
                <kanban create="false" group_create="false">
                    <field name="survey_id"/>
                    <field name="create_date"/>
                    <field name="partner_id"/>
                    <field name="email"/>
                    <field name="state"/>
                    <templates>
                        <t t-name="kanban-box">
                            <div t-attf-class="oe_kanban_global_click">
                                <div class="o_kanban_record_top">
                                    <div class="o_kanban_record_headings oe_kanban_top_left">
                                        <strong class="o_kanban_record_title"><t t-esc="record.survey_id.value"/></strong>
                                    </div>
                                    <div class="oe_kanban_bottom_right mr4">
                                        <button name="open_survey" string="Open" type="object" class="btn btn-primary" style="font-size: 10px; padding: 2px 4px;" />
                                    </div>
                                </div>
                                <div class="o_kanban_record_bottom">
                                    <div class="oe_kanban_bottom_left">
                                        <field name="create_date"/>
                                    </div>
                                    <div class="oe_kanban_bottom_right mr4">
                                        <field name="state" widget="label_selection" options="{'classes': {'new': 'default', 'done': 'success', 'in_progress':'warning'}}"/>
                                    </div>
                                </div>
                            </div>
                        </t>
                    </templates>
                </kanban>
            </field>
        </record>

        <!-- personal template -->
        <record id="view_survey_survey_personal_tree_custom" model="ir.ui.view">
            <field name="name">survey.survey.personal.tree.custom</field>
