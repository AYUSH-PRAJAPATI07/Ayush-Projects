<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record id="seq_patient_case_id" model="ir.sequence">
            <field name="name">Patient Case Sequence</field>
            <field name="code">patient.case</field>
            <field name="prefix">PCID/%(y)s/</field>
            <field name="padding">4</field>
        </record>

        <record id="patient-case-backend-sequence" model="ir.sequence">
            <field name="name">Patient Case Backend Sequence</field>
            <field name="code">patient.case.backend</field>
            <field name="prefix">ID-</field>
            <field name="padding">1</field>
        </record>

        <record id="seq_contact_attempt" model="ir.sequence">
            <field name="name">Contact Attempt</field>
            <field name="code">contact.attempt.seq</field>
            <field name="prefix">CA</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>